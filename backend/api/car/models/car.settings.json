{
  "kind": "collectionType",
  "collectionName": "cars",
  "info": {
    "name": "car"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "model": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "slug": {
      "type": "uid"
    },
    "photos": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "reviews": {
      "via": "car",
      "collection": "review"
    }
  }
}
